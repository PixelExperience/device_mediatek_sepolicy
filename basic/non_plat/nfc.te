# NFC Hals
typeattribute nfc data_between_core_and_vendor_violators;
allow nfc vendor_hal_nfc_default:binder { call transfer };
allow nfc nfc_data_file:file { open };
allow nfc vendor_data_file:dir { rw_dir_perms };
allow nfc vendor_data_file:file { create_file_perms rw_file_perms };
dontaudit nfc init:binder { call transfer };
